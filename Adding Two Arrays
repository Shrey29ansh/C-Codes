#include<iostream>
using namespace std;
int main()
{
	int a[2][2],b[2][2],c[2][2],i,j;
	cout<<"enter 1st array elements";
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
			cin>>a[i][j];
		}
		
	}
	cout<<"enter 2nd array elements";
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
			cin>>b[i][j];	
		}
	}
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
			c[i][j]=a[i][j]+b[i][j];
			cout<<c[i][j];
			if(j==1)
			cout<<"\n";
			else
			cout<<" , ";
		}
	}
	

	return 0;
}
